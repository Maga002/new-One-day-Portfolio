                <div class="topbar_inner">
                    <div class="logo">
                        <a href="#">
                            <img class="light" src="~/img/logo/logo.png" alt="" />
                            <img class="dark" src="~/img/logo/dark.png" alt="" />
                        </a>
                    </div>
                    <div class="menu">
                        <div class="links">
                            <ul class="anchor_nav">
                                <li class="current">
                                    <a href="#home">
                                        <span class="first">Home</span>
